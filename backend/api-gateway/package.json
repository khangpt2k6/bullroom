    "@types/node": "^20.10.6",
    "@types/amqplib": "^0.10.4",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "nodemon": "^3.0.2"
  }
